<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PlanetController">
   <children>
      <GridPane prefHeight="500.0" prefWidth="500.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="155.0" minHeight="0.0" prefHeight="44.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="320.0" minHeight="10.0" prefHeight="174.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="219.0" minHeight="10.0" prefHeight="218.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Text fx:id="planetName" strokeType="OUTSIDE" strokeWidth="0.0" text="Planet Name" GridPane.halignment="CENTER">
               <font>
                  <Font size="48.0" />
               </font>
            </Text>
            <Text fx:id="planetInfo" strokeType="OUTSIDE" strokeWidth="0.0" text="Planet Info" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <GridPane GridPane.rowIndex="2">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="mpButton" mnemonicParsing="false" onAction="#onPressMarketPlace" text="MarketPlace" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
                  <Button fx:id="travelButton" mnemonicParsing="false" onAction="#onPressTravel" text="Travel" GridPane.halignment="CENTER" />
                  <Button fx:id="shipButton" mnemonicParsing="false" onAction="#onPressShipyard" text="Shipyard" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                  <Button fx:id="bankButton" mnemonicParsing="false" onAction="#onPressBank" text="Bank" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </children>
      </GridPane>
   </children>
</AnchorPane>
